<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMPractica2"
        xmlns:ViewModel="clr-namespace:MVVMPractica2.ViewModel" x:Class="MVVMPractica2.View.DialogWindow"
        mc:Ignorable="d" Height="150" Width="250" Title="{Binding Titol}">
    <Window.DataContext>
        <ViewModel:DialogWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="16,30,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="111" Text="{Binding Tb}"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="16,68,0,0" VerticalAlignment="Top" Width="50" Command="{Binding CancelCommand}" />
        <Button Content="Ok" HorizontalAlignment="Left" Margin="94,68,0,0" VerticalAlignment="Top" Width="33" RenderTransformOrigin="0.941,0.526" Command="{Binding OkCommand}"/>
    </Grid>
</Window>
